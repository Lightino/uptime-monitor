{
  "name": "uptime-monitor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "serve": "vite --host",
    "build": "vite build --base=/",
    "preview": "vite preview",
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS=--openssl-legacy-provider npm-run-all --parallel serve server",
    "server": "nodemon src/server",
    "knex": "knex --knexfile ./src/server/db/knexfile.js",
    "knex:dev": "cross-env NODE_ENV=development NODE_OPTIONS=--openssl-legacy-provider yarn knex"
  },
  "dependencies": {
    "@iconify-json/tabler": "^1.2.18",
    "@iconify/tailwind4": "^1.0.6",
    "@tailwindcss/vite": "^4.1.7",
    "bookshelf": "^1.2.0",
    "bookshelf-json-columns": "^3.0.0",
    "bookshelf-modelbase": "^2.11.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "dotenv-flow": "^4.1.0",
    "express": "4.20",
    "flyonui": "^2.1.0",
    "knex": "^3.1.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "pg": "^8.16.0",
    "pinia": "^3.0.2",
    "recharts": "^2.15.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwindcss": "^4.1.7",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "vite": "^6.3.5"
  }
}
